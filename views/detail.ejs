<div class="editor-container">
  <form action="/details/create/<%= campground._id %>" method="POST">
    <input type="hidden" name="details">
    <div id="editor">
      <p>Hello World!</p>
      <p>This is where you can submit text to <strong>create</strong> a file!</p>
      <p><br></p>
    </div>
    <div class="editor-button-container">
      <button type="submit" class="ui red button">Create File</button>
    </div>
  </form>
</div>

<a href="">View Details</a>

<form class="" action="/details/<%= campground._id %>/new" method="post">
  <textarea type="text" name="detail"></textarea>
  <button type="submit" class="ui red button">Create File</button>
</form>

<script src="https://cdn.quilljs.com/1.3.4/quill.js"></script>
<script src="/js/quill.js"></script>
<script src="/js/details.js"></script>
